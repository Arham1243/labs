{
    "name": "phoenix-frontend",
    "version": "4.3.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
        "configure-husky": "npx husky install && npx husky add .husky/pre-commit \"npx --no-install lint-staged\"",
        "test": "vitest run --maxConcurrency=1 --pool=threads",
        "coverage": "vitest run --coverage",
        "prepare": "husky"
    },
    "dependencies": {
        "@casl/ability": "^6.7.2",
        "@casl/vue": "^2.2.2",
        "@fullcalendar/core": "^6.1.8",
        "@fullcalendar/daygrid": "^6.1.18",
        "@fullcalendar/interaction": "^6.1.19",
        "@fullcalendar/timegrid": "^6.1.19",
        "@fullcalendar/vue3": "^6.1.18",
        "@intlify/unplugin-vue-i18n": "^11.0.1",
        "@vue-js-cron/light": "^5.1.0",
        "axios": "^1.12.2",
        "chart.js": "4.5.1",
        "cronstrue": "^3.9.0",
        "firebase": "^12.4.0",
        "jszip": "^3.10.1",
        "libphonenumber-js": "^1.12.24",
        "lint-staged": "^16.2.6",
        "lodash": "^4.17.21",
        "moment-timezone": "^0.6.0",
        "pinia": "^3.0.3",
        "primeflex": "^3.3.1",
        "primeicons": "^7.0.0",
        "primevue": "3.30.2",
        "quill": "^2.0.3",
        "swiper": "^12.0.3",
        "uuid": "^13.0.0",
        "vue": "^3.5.22",
        "vue-i18n": "^11.1.12",
        "vue-router": "^4.6.3",
        "vue-tel-input": "^9.5.0",
        "vue3-cookies": "^1.0.6",
        "vue3-otp-input": "^0.5.40",
        "xlsx": "npm:@e965/xlsx@0.20.3"
    },
    "devDependencies": {
        "@rushstack/eslint-patch": "^1.14.0",
        "@testing-library/vue": "^8.1.0",
        "@vitejs/plugin-vue": "^6.0.1",
        "@vitest/coverage-istanbul": "^4.0.1",
        "@vitest/ui": "^4.0.1",
        "@vue/eslint-config-prettier": "^10.2.0",
        "@vue/test-utils": "^2.4.6",
        "eslint": "^9.38.0",
        "eslint-plugin-vue": "^10.5.1",
        "happy-dom": "^20.0.8",
        "husky": "^9.1.7",
        "jsdom": "^27.0.1",
        "prettier": "^3.6.2",
        "sass": "^1.93.2",
        "typescript": "^5.9.3",
        "vite": "^7.1.12",
        "vitest": "^4.0.1"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*": "prettier --write"
    },
    "optionalDependencies": {
        "@esbuild/linux-x64": "^0.25.11"
    }
}
